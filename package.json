{
  "name": "thepcbstudio",
  "private": true,
  "description": "thepcbstudio website",
  "version": "0.1.0",
  "author": "Udit Desai <desaiuditd@gmail.com>",
  "dependencies": {
    "gatsby": "^2.15.1",
    "gatsby-image": "^2.2.15",
    "gatsby-plugin-offline": "^3.0.0",
    "gatsby-plugin-react-helmet": "^3.1.5",
    "gatsby-plugin-sharp": "^2.2.18",
    "gatsby-source-filesystem": "^2.1.18",
    "gatsby-transformer-sharp": "^2.2.12",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "gh-pages": "^2.1.1",
    "husky": "^3.0.4",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "GATSBY_TELEMETRY_DISABLED=1 gatsby build",
    "develop": "GATSBY_TELEMETRY_DISABLED=1 gatsby develop",
    "deploy": "GATSBY_TELEMETRY_DISABLED=1 gatsby build && gh-pages -d public -b master",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "GATSBY_TELEMETRY_DISABLED=1 gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing \""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
