{
  "compilerOptions": {
    "baseUrl": ".",
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "lib": [ "DOM", "DOM.Iterable", "ESNext" ],
    "module": "esnext",
    "moduleResolution": "bundler",
    "paths": {
      "~/*": [ "./app/*" ]
    },
    "rootDirs": [ ".", "./.react-router/types" ],
    "target": "ESNext",
    "types": [ "node", "vite/client" ]
  },
  "include": [ "**/*", "**/.server/**/*", "**/.client/**/*", ".react-router/types/**/*" ]
}
